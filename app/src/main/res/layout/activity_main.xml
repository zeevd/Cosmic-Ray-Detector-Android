<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    >


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_dropbox_settings"
        android:id="@+id/button_dropbox_settings"
        android:onClick="onClickDropboxActivity"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Graph Data"
        android:id="@+id/button_graph_data"
        android:onClick="onClickGraphData"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/button_dropbox_settings"
        android:layout_alignStart="@+id/button_dropbox_settings" />

    <ImageView
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:id="@+id/imageView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/sbu"
        android:layout_weight="1" />

    <ImageView
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:id="@+id/imageView2"
        android:src="@drawable/bnl"
        android:layout_weight="1"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
